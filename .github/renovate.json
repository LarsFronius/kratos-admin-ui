{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabled": true,
  "extends": [
    "config:base"
  ],
  "assignees": [
    "dfoxg"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "excludePackagePatterns": [
        "^@fluentui",
        "^@types",
        "^react",
        "typescript",
        "^@testing",
      ],
      "groupName": "deps"
    }
  ]
}