{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabled": true,
  "extends": [
    "config:base"
  ],
  "assignees": [
    "dfoxg"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "^@fluentui"
      ],
      "groupName": "fluentui"
    },
    {
      "matchPackagePatterns": [
        "^@types"
      ],
      "groupName": "types"
    },
    {
      "matchPackagePatterns": [
        "^react"
      ],
      "groupName": "react"
    },
    {
      "matchPackagePatterns": [
        "^@testing"
      ],
      "groupName": "testing"
    }
  ]
}